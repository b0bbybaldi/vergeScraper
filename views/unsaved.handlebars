<div class="container">
  <div id="wrapper">
    {{#each articles}} {{#unless saved}} {{#if link}}
    <a href="{{link}}" target="_blank">
      <h2>{{title}}</h2>
    </a>
    {{else}}
    <h3>{{title}}</h3>
    {{/if}}
    <button id="saveArticle" data-id="{{_id}}">Save Article</button>
    {{/unless}} {{/each}}
  </div>
</div>